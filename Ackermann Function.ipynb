   "source": [
    "def acarmann(m,n):\n",
    "    if m==0:\n",
    "        return n+1\n",
    "    elif m>0 and n==0:\n",
    "        return acarmann(m-1,1)\n",
    "    elif m>0 and n>0:\n",
    "        return acarmann(m-1,acarmann(m,n-1))"
   ]
  },
